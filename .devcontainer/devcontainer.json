{
  "name": "Node.js",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "18-bullseye"
    }
  },
  "customizations": {
    "codespaces": {
      "repositories": {
        "UseAleron/design": {
          "permissions": {
            "contents": "read"
          }
        }
      }
    },
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Vue.volar"
      ]
    }
  },
  "postCreateCommand": "npm install",
  "portsAttributes": {
    "5173": { "label": "Development server" }
  },
  "remoteUser": "node"
}
